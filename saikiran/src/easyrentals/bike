package com.easyRentals.models;

import java.time.LocalDate;

public class Bike {
	
	private int bikeId;
	private String serialNumber;
	private String bikeName;
	private double rentalPrice;
	private String companyName;
	private LocalDate rentDate;
	public Bike(int bikeId, String serialNumber, String bikeName, double rentalPrice, String companyName,
			LocalDate rentDate) {
		super();
		this.bikeId = bikeId;
		this.serialNumber = serialNumber;
		this.bikeName = bikeName;
		this.rentalPrice = rentalPrice;
		this.companyName = companyName;
		this.rentDate = rentDate;
	}
	public int getBikeId() {
		return bikeId;
	}
	public void setBikeId(int bikeId) {
		this.bikeId = bikeId;
	}
	public String getSerialNumber() {
		return serialNumber;
	}
	public void setSerialNumber(String serialNumber) {
		this.serialNumber = serialNumber;
	}
	public String getBikeName() {
		return bikeName;
	}
	public void setBikeName(String bikeName) {
		this.bikeName = bikeName;
	}
	public double getRentalPrice() {
		return rentalPrice;
	}
	public void setRentalPrice(double rentalPrice) {
		this.rentalPrice = rentalPrice;
	}
	public String getCompanyName() {
		return companyName;
	}
	public void setCompanyName(String companyName) {
		this.companyName = companyName;
	}
	public LocalDate getRentDate() {
		return rentDate;
	}
	public void setRentDate(LocalDate rentDate) {
		this.rentDate = rentDate;
	}
	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return "BikeId: "+this.bikeId+", Bike SerialNumber: "+this.serialNumber+", Bike name: "+this.bikeName+", Company Name: "+this.companyName+", Rental price: "+this.rentalPrice;
	}
}
